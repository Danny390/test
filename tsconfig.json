{
    "compilerOptions": {
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noEmit": true,
      "noFallthroughCasesInSwitch": true,
      "jsx": "react-jsx"
    },
    "include": [
      "src",
    ]
  }